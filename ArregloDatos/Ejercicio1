
//Declara un arreglo, y llénalo con datos. Puedes utilizar el ejemplo anterior. 
//Luego imprime en pantalla las posiciones de memoria correspondientes a cada posición.

#include <stdio.h>

int main() {
    int numeros[5] = {10, 20, 30, 40, 50};

    // se muestran los valores y sus direcciones
    for(int i = 0; i < 5; i++) {
        printf("numeros[%d] = %d \n Dirección: %p\n", i, numeros[i], &numeros[i]);
    }

    return 0;
}